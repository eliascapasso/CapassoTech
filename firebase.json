{
  "hosting": {
    "public": "dist/capassotech",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "redirects": [
      {
        "source": "http://www.capasso.tech",
        "destination": "https://capasso.tech",
        "type": 301
      },
      {
        "source": "http://capasso.tech",
        "destination": "https://capasso.tech",
        "type": 301
      },
      {
        "source": "https://www.capasso.tech",
        "destination": "https://capasso.tech",
        "type": 301
      }
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}